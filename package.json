{
  "name": "gatsby-starter-ts",
  "description": "A TypeScript starter for Gatsby that includes all you need to build amazing projects",
  "version": "1.0.0",
  "private": true,
  "author": "Jo√£o Pedro Schmitz <hey@joaopedro.dev> (https://joaopedro.dev)",
  "license": "MIT",
  "keywords": [
    "gatsby",
    "starter",
    "typescript"
  ],
  "scripts": {
    "start": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "type-check": "tsc",
    "lint": "eslint --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\"",
    "format": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\" --write"
  },
  "lint-staged": {
    "./src/**/*.{ts,js,jsx,tsx}": [
      "yarn lint --fix",
      "yarn format"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.4.0",
    "@emotion/styled": "^11.3.0",
    "@fontsource/bubblegum-sans": "^4.4.5",
    "@fontsource/epilogue": "^4.4.5",
    "@fontsource/ibm-plex-mono": "^4.4.5",
    "@fontsource/josefin-sans": "^4.4.5",
    "@fontsource/poppins": "^4.4.5",
    "@loadable/component": "^5.15.0",
    "@reach/router": "^1.3.4",
    "@types/react-lazy-load-image-component": "^1.5.2",
    "@types/tinycolor2": "^1.4.3",
    "facepaint": "^1.2.1",
    "framer-motion": "^4.1.17",
    "gatsby": "^3.8.0",
    "gatsby-plugin-emotion": "^6.8.0",
    "gatsby-plugin-graphql-codegen": "^3.0.0",
    "gatsby-plugin-loadable-components-ssr": "^3.2.0",
    "gatsby-plugin-netlify-cms": "^5.10.0",
    "gatsby-source-filesystem": "^3.10.0",
    "gatsby-transformer-remark": "^4.7.0",
    "netlify-cms-app": "^2.15.30",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.2.0",
    "react-lazy-load-image-component": "^1.5.1",
    "react-responsive": "^8.2.0",
    "tinycolor2": "^1.4.2",
    "tsconfig-paths-webpack-plugin": "^3.5.1",
    "typewriter-effect": "^2.18.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@types/facepaint": "^1.2.2",
    "@types/node": "^15.12.4",
    "@types/react": "^17.0.11",
    "@types/react-dom": "^17.0.8",
    "@typescript-eslint/eslint-plugin": "^4.28.0",
    "@typescript-eslint/parser": "^4.28.0",
    "babel-plugin-root-import": "^6.6.0",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.29.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-root-import": "^1.0.4",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.2",
    "typescript": "^4.3.4"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
